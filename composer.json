{
    "name": "tavy315/sylius-recommendations-plugin",
    "type": "sylius-plugin",
    "description": "Recommendations plugin for Sylius.",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "babdev/pagerfanta-bundle": "^4.5",
        "sylius/channel": "^1.11",
        "sylius/core": "^1.11",
        "sylius/core-bundle": "^1.11",
        "sylius/customer": "^1.11",
        "sylius/locale": "^1.11",
        "sylius/product": "^1.11",
        "sylius/product-bundle": "^1.11",
        "sylius/resource-bundle": "^1.11",
        "sylius/ui-bundle": "^1.11"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-doctrine": "^1.5",
        "sylius-labs/coding-standard": "^4.4",
        "vimeo/psalm": "^5.26"
    },
    "autoload": {
        "psr-4": {
            "Tavy315\\SyliusRecommendationsPlugin\\": "src/"
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
